    style: containerStyle,
    className: `ReactFlipCard__container ${containerCss} `,
    onMouseEnter: e => {
      if (flipTrigger === FlipTrigger.onHover) {
        setIsFlipped(!isFlipped);
      }
      onMouseEnter(e);
    },
    onMouseLeave: e => {
      if (flipTrigger === FlipTrigger.onHover) {
